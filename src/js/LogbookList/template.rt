<div>
	<h2>Hallo Du</h2>
	<table>
		<thead>
			<tr>
				<th>Date</th>
				<th>Title</th>
				<th>Location</th>
				<th>Type</th>
				<th>Time</th>
				<th>Depth</th>
			</tr>
		</thead>
		<tbody>
			<tr rt-repeat="row in this.state.rows" key="{row.index}">
				<td>{row.index}</td>
				<td>{row.value}</td>
			</tr>
		</tbody>
	</table>
	<a href="javascript:void(0)" onClick="()=>this.test()">Testbutton</a>
</div>